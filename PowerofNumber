import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int x = sc.nextInt();
        int ans = 1;
        while (x > 0) {
        	ans = ans * n;	
        	x-- ;      	
        }
       
        System.out.println(ans);
   
    }
}




