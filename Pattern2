import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int i =1;
        int counter = 1;
        while (i <= n ) {
        	int j = 1;
        	while(j<=counter) {        		
        	System.out.print("*");     	
        	j++;	
        }
        	counter++;
        	System.out.println();
        	i++;
        }
          
    }
}




